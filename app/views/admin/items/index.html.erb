
  　　 <h2>商品一覧</h2>
      <table class="table table-hover">
        <thead>
          <tr>
            <th>商品ID</th>
            <th>商品名</th>
            <th>税抜価格</th>
            <th>ジャンル</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
        <% @items.each do |item| %>
          <tr>
            <td><%= item.id %></td>
            <td><%= link_to item.name, admin_item_path(item.id) %></td>
            <td><%= item.tax_out_price.to_s(:delimited) %></td>
            <td><%#= item.tax_out_priceの税抜金額算出 %></td>
            <td><%#= item.genre_name %></td>
            <td><%= item.is_onsele %></td>
          </tr>
        <% end %>
        </tbody>
      </table>


