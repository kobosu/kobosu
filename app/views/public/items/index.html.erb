    <h2>商品一覧</h2>
  　   <table class="table table-hover">
  　     <tbody>
  　       <% @items.each do |item| %>
          <tr>
            <th><%#= link_to(item) do %>
                <%#= image_tag item.get_profile_image, size:"50x50" %>
                <%# end %>
            </th>
            <th><%= link_to item.name, item_path(item.id) %></th>
            <th>￥<%= item.tax_out_price.to_s(:delimited) %></th>
          </tr>
          <% end %>
        </tbody>
      </table>
